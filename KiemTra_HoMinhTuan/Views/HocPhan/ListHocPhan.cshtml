@model IEnumerable<KiemTra_HoMinhTuan.Models.HocPhan>

@{
    ViewBag.Title = "ListHocPhan";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ListHocPhan</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenHP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SoTinChi)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenHP)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SoTinChi)
        </td>
        <td>
            @Html.ActionLink("Đăng Ký", "Đăng Ký", "HocPhan")
        </td>
    </tr>
}

</table>
